{
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "target": "es2017",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "incremental": true,
      "skipLibCheck": true,
      "strict": true,
      "paths": {
        "src/*": ["./src/*"]
      }
    }
  }
  